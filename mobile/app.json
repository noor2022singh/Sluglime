{
  "expo": {
    "name": "SlugLime",
    "slug": "sluglime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "sluglime",
    "linking": {
      "prefixes": [
        "sluglime://",
        "https://sluglime.in"
      ],
      "config": {
        "screens": {
          "posts": {
            "path": "posts/:id"
          }
        }
      }
    },
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "entryPoint": "./App.js",
    "ios": {
      "supportsTablet": true,
      "associatedDomains": [
        "applinks:sluglime.in",
        "applinks:sluglime.in"
      ],
      "bundleIdentifier": "com.anonymous.sluglime",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "navigationBarColor": "#000000",
      "statusBarColor": "#000000",
      "package": "com.anonymous.sluglime",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "https",
              "host": "sluglime.in",
              "pathPrefix": "/posts"
            },
            {
              "scheme": "https",
              "host": "sluglime.in",
              "pathPrefix": "/p"
            }
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7894f219-a3bf-48b6-bb77-672540893030"
      }
    }
  }
}
